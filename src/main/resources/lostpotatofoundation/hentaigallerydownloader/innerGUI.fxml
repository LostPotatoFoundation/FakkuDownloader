<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="pane" prefHeight="172.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lostpotatofoundation.hentaigallerydownloader.gui.MainController">
   <children>
      <TextField fx:id="linkText" layoutX="25.0" layoutY="50.0" prefHeight="25.0" prefWidth="344.0" promptText="Enter link" />
      <Button fx:id="startButton" layoutX="150.0" layoutY="95.0" mnemonicParsing="false" onAction="#startDownload" text="Download URL" />
      <ProgressBar fx:id="progressBar" layoutX="25.0" layoutY="136.0" prefHeight="18.0" prefWidth="336.0" progress="0.0" />
      <ImageView fx:id="image" fitHeight="150.0" fitWidth="110.0" layoutX="476.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true" />
      <TextField fx:id="titlePart" editable="false" layoutX="24.0" layoutY="14.0" prefHeight="25.0" prefWidth="344.0" />
      <Button fx:id="parseLinkFile" layoutX="24.0" layoutY="95.0" mnemonicParsing="false" onAction="#parseLinksFromFile" text="Parse Links List" />
      <Button fx:id="updateFavs" layoutX="280.0" layoutY="95.0" mnemonicParsing="false" onAction="#updateFavourites" text="Update Favs" />
      <Button fx:id="changeTheme" layoutX="381.0" layoutY="133.0" mnemonicParsing="false" onAction="#changeTheme" text="Theme" />
   </children>
</Pane>
